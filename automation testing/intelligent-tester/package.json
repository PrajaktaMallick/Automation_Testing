{
  "name": "intelligent-web-tester",
  "version": "1.0.0",
  "description": "Intelligent automated web tester with MCP integration and natural language processing",
  "main": "backend/main.py",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "frontend:dev": "cd frontend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install",
    "setup": "python -m pip install -r backend/requirements.txt && cd frontend && npm install",
    "test": "cd backend && python -m pytest tests/",
    "playwright:install": "cd backend && python -m playwright install"
  },
  "keywords": [
    "automation",
    "testing",
    "playwright",
    "mcp",
    "ai",
    "web-testing",
    "intelligent-testing"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  }
}
